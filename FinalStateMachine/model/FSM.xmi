<?xml version="1.0" encoding="UTF-8"?>
<finalStateMachine:FSM
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:finalStateMachine="http://www.example.org/finalStateMachine"
    xsi:schemaLocation="http://www.example.org/finalStateMachine finalStateMachine.ecore">
  <transition
      input="//@state.1"
      output="//@state.0"/>
  <transition
      input="//@state.0"
      output="//@state.2"/>
  <transition
      input="//@state.2"
      output="//@state.3"/>
  <state name="s1"/>
  <state xsi:type="finalStateMachine:InitialState"
      name="init"/>
  <state name="s2"/>
  <state xsi:type="finalStateMachine:FinalState"
      name="final"/>
</finalStateMachine:FSM>
